package com.duval.components;

public class T2Fault {
	int xorigin;
	int yorigin;
	
	public T2Fault(int xorigin, int yorigin) {
		super();
		this.xorigin = xorigin;
		this.yorigin = yorigin;
	}
	


	public int[] getT3x(int xorigin) {
		return new int[]{
				xorigin + 24,
				xorigin + -1,
				xorigin + -6,
				xorigin + 1
				};
	}
	
	public int[] getT3y(int yorigin) {
		return new int[]{
				yorigin - -30,
				yorigin - -2,
				yorigin - -4,
				yorigin - -32
				};
	}
	
		
	public int len() {
		return 4;
	}
	
}
