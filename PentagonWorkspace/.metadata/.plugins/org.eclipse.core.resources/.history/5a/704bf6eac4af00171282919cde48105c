package com.duval.components;

public class T1Fault {
	int xorigin;
	int yorigin;
	
	public T1Fault(int xorigin, int yorigin) {
		super();
		this.xorigin = xorigin;
		this.yorigin = yorigin;
	}
	



	public int[] getT1x(int xorigin) {
		return new int[]{
				xorigin + 23,
				xorigin + -6,
				xorigin + -1,
				xorigin + 0,
				xorigin + -35
				};
	}
	
	public int[] getT1y(int yorigin) {
		return new int[]{
				yorigin - -32,
				yorigin - -4,
				yorigin - -2,
				yorigin - 2,
				yorigin - 3
				};
	}

			
	public int len() {
		return 3;
	}
	
}
